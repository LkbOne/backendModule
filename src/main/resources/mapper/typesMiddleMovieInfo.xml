<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.hugh.life.dao.api.TypesMiddleMovieInfoDao">
    <resultMap id="BaseResultMap" type="typesMiddleMovieInfoEntity">
        <id column="id" property="id"/>
        <result column="movie_info_id" property="movieInfoId"/>
        <result column="types_id" property="typesId"/>
        <result column="create_time" property="createTime"/>
    </resultMap>

    <insert id="add" parameterType="peopleInfoMiddleMovieInfoEntity">
        INSERT INTO types_middle_movie_info(
        `id`,
        `movie_info_id`,
        `types_id`,
        `create_time`
        )
        VALUES (
        #{id},
        #{movieInfoId},
        #{typesId},
        now()
        )
    </insert>
</mapper>