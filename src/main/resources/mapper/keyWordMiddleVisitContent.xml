<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.hugh.life.dao.api.KeyWordMiddleVisitContentDao">
    <resultMap id="BaseResultMap" type="keyWordMiddleVisitContentEntity">
        <id column="id" property="id"/>
        <result column="key_word_id" property="keyWordId"/>
        <result column="visit_content_id" property="visitContentId"/>
        <result column="create_time" property="createTime"/>
        <result column="last_modify_time" property="lastModifyTime"/>
    </resultMap>

    <insert id="add" parameterType="keyWordMiddleVisitContentEntity">
        INSERT INTO key_word_middle_visit_content(
        `id`,
        `key_word_id`,
        `visit_content_id`,
        `create_time`,
        `last_modify_time`
        )
        VALUES (
        #{id},
        #{keyWordId},
        #{visitContentId},
        now(),
        now()
        )
    </insert>



</mapper>